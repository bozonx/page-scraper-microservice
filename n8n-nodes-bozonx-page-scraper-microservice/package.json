{
  "name": "n8n-nodes-bozonx-page-scraper-microservice",
  "version": "1.6.0",
  "description": "n8n node for Page Scraper microservice - extract structured content, retrieve HTML, and process URLs in batches",
  "license": "MIT",
  "keywords": [
    "n8n-community-node-package",
    "page-scraper",
    "web-scraping",
    "content-extraction",
    "bozonx"
  ],
  "author": {
    "name": "Ivan K"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bozonx/page-scraper-microservice"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release:n8n": "n8n-node release",
    "prepublishOnly:n8n": "n8n-node prerelease",
    "publish:npm": "pnpm build && pnpm publish --access public"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/PageScraperApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/PageScraper/PageScraper.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.16.0",
    "eslint": "9.39.1",
    "n8n-workflow": "^1.118.1",
    "prettier": "3.6.2",
    "release-it": "^19.0.6",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}