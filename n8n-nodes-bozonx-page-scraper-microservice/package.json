{
  "name": "n8n-nodes-bozonx-page-scraper-microservice",
  "version": "0.1.0",
  "description": "n8n nodes for BozonX Page Scraper microservice (params and start batch)",
  "license": "MIT",
  "keywords": [
    "n8n-community-node-package",
    "page-scraper",
    "bozonx"
  ],
  "author": {
    "name": "Ivan K"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release:n8n": "n8n-node release",
    "prepublishOnly:n8n": "n8n-node prerelease",
    "publish:npm": "pnpm build && pnpm publish --access public"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/BozonxMicroservicesApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/PageScraper/PageScraper.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2",
    "n8n-workflow": "*"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
